<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LinearDot">
        <attr name="normalDrawable" format="color|reference" />
        <attr name="normalDimension" format="dimension" />
        <attr name="selectedDrawable" format="color|reference" />
        <attr name="selectedDimension" format="dimension" />
        <attr name="intervalDimension" format="dimension" />
        <attr name="dotCount" format="integer" />
    </declare-styleable>

    <declare-styleable name="LinearStars">
        <attr name="starMaxNum" format="integer" />
        <attr name="hollowStarDrawable" format="color|reference" />
        <attr name="solidStarDrawable" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="RegularTriangleView">
        <attr name="triangleRadius" format="dimension" />
        <attr name="triangleFillColor" format="color" />
    </declare-styleable>

    <declare-styleable name="PathDrawView">
        <attr name="pathDrawFillColor" format="color" />
    </declare-styleable>

    <declare-styleable name="PasswordView">
        <attr name="cellWidth" format="dimension" />
        <attr name="cellHeight" format="dimension" />
        <attr name="circleRadius" format="dimension" />
        <attr name="passwordSpaing" format="dimension" />
        <attr name="passwordLength" format="integer" />
    </declare-styleable>

    <declare-styleable name="PasswordEditText">
        <attr name="borderStrokeColor" format="color" />
        <attr name="passwordBackground" format="color" />
        <attr name="borderStrokeWidth" format="dimension" />
        <attr name="borderRadius" format="dimension" />
        <attr name="textWordColor" format="color" />
        <attr name="textWordRadius" format="dimension" />
        <attr name="textWordLength" format="integer" />
    </declare-styleable>

    <declare-styleable name="WheelView">
        <attr name="unitHeight" format="dimension" />
        <attr name="unitLeftMargin" format="dimension" />
        <attr name="itemNumber" format="integer" />
        <attr name="normalTextColor" format="color|reference" />
        <attr name="normalTextSize" format="dimension" />
        <attr name="selectedTextColor" format="color|reference" />
        <attr name="selectedTextSize" format="dimension" />
        <attr name="unitTextColor" format="color|reference" />
        <attr name="unitTextSize" format="dimension" />
        <attr name="lineColor" format="color|reference" />
        <attr name="lineHeight" format="dimension" />
        <attr name="maskHeight" format="dimension" />
        <attr name="noEmpty" format="boolean" />
        <attr name="isEnable" format="boolean" />
        <attr name="unitText" format="string|reference" />
    </declare-styleable>

    <declare-styleable name="MarqueeView">
        <attr name="speed" format="integer" />
        <attr name="interval" format="integer" />
    </declare-styleable>

    <declare-styleable name="ExpandableItemView">
        <attr name="expandleView" format="reference" />
        <attr name="NonExpandleView" format="reference" />
        <!--<attr name="isHeaderParallax" format="boolean" />-->
    </declare-styleable>


    <declare-styleable name="AlphaLinearlayout">
        <attr name="alphaSpeed" format="float" />
    </declare-styleable>

</resources>